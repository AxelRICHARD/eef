<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

 <plugin>

  <!-- EEF extensions -->
  <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionProvider">
      <PropertiesEditionComponentProvider
            providerClass="org.eclipse.emf.eef.eefnr.providers.EefnrPackagePropertiesEditionProvider">
      </PropertiesEditionComponentProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPolicyProvider">
      <PropertiesEditionPolicyProvider
            providerClass="org.eclipse.emf.eef.eefnr.providers.EefnrPackagePropertiesEditionPolicyProvider">
      </PropertiesEditionPolicyProvider>
   </extension>

   <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.eclipse.emf.eef.eefnr.properties">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.emf.eef.eefnr.properties">
         <propertyTab
               label="Base"
               category="default"
               id="Base">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.eclipse.emf.eef.eefnr.properties">
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.Root"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.Root">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.TotalSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.TotalSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.TextSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.TextSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.CheckboxSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.CheckboxSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.TextareaSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.TextareaSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.RadioSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.RadioSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.EObjectFlatComboViewerSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.EObjectFlatComboViewerSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.ReferencesTableSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.ReferencesTableSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.EMFComboViewerSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.EMFComboViewerSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.MultiValuedEditorSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.MultiValuedEditorSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.TableCompositionEditorSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.TableCompositionEditorSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.AdvancedReferencesTableSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.AdvancedReferencesTableSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.AdvancedEObjectFlatComboViewerSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.AdvancedEObjectFlatComboViewerSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.AdvancedTableCompositionEditorSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.AdvancedTableCompositionEditorSample">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.emf.eef.eefnr.section.FlatReferencesTableSample"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.eefnr.FlatReferencesTableSample">
             </input>
         </propertySection>
      </propertySections>
   </extension>
 </plugin>
