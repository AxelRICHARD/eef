<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://wwww.obeo.fr/dsl/worldcupforecast/1.0.0"
            class="org.eclipse.worldcupforecast.provider.WorldcupforecastItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>
     <!-- EEF extensions -->
  <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionProvider">
      <PropertiesEditionComponentProvider
            providerClass="org.eclipse.worldcupforecast.providers.WorldcupforecastPackagePropertiesEditionProvider">
      </PropertiesEditionComponentProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPolicyProvider">
      <PropertiesEditionPolicyProvider
            providerClass="org.eclipse.worldcupforecast.providers.WorldcupforecastPackagePropertiesEditionPolicyProvider">
      </PropertiesEditionPolicyProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.eclipse.worldcupforecast.providers.WorldcupforecastPropertiesEditionPartProviderCustom">
      </PropertiesEditionPartProvider>
   </extension>

   <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.eclipse.worldcupforecast.properties">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.worldcupforecast.properties">
         <propertyTab
               label="Base"
               category="default"
               id="Base">
         </propertyTab>
         <propertyTab
               label="WorldCupForecastGroup"
               category="extended"
               id="WorldCupForecastGroup"
               afterTab="WorldCupForecastGroup">
         </propertyTab>
         <propertyTab
               label="WorldCupForecastPLayer"
               category="extended"
               id="WorldCupForecastPLayer"
               afterTab="Base">
         </propertyTab>
         <propertyTab
               label="WorldCupForecastMatchDay"
               category="extended"
               id="WorldCupForecastMatchDay"
               afterTab="WorldCupForecastMatchDay">
         </propertyTab>
         <propertyTab
               label="WorldCupForecastPlayersRating"
               category="extended"
               id="WorldCupForecastPlayersRating"
               afterTab="WorldCupForecastPlayersRating">
         </propertyTab>
         <propertyTab
               label="GroupeStageMatch"
               category="extended"
               id="GroupeStageMatch"
               afterTab="GroupeStageMatch">
         </propertyTab>
         <propertyTab
               label="OtherKnockoutStageGame"
               category="extended"
               id="OtherKnockoutStageGame"
               afterTab="OtherKnockoutStageGame">
         </propertyTab>
         <propertyTab
               label="ThirdPlaceMatch"
               category="extended"
               id="ThirdPlaceMatch"
               afterTab="ThirdPlaceMatch">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.eclipse.worldcupforecast.properties">
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.Forecast"
               tab="Base">
             <input
                    type="org.eclipse.worldcupforecast.Forecast">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.Team"
               tab="Base">
             <input
                    type="org.eclipse.worldcupforecast.Team">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.WorldCupGroup"
               tab="Base">
             <input
                    type="org.eclipse.worldcupforecast.WorldCupGroup">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.Player"
               tab="Base">
             <input
                    type="org.eclipse.worldcupforecast.Player">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.FinalScore"
               tab="Base">
             <input
                    type="org.eclipse.worldcupforecast.FinalScore">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.MatchDay"
               tab="Base">
             <input
                    type="org.eclipse.worldcupforecast.MatchDay">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.ForecastDay"
               tab="Base">
             <input
                    type="org.eclipse.worldcupforecast.ForecastDay">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.WorldCupForecast"
               tab="WorldCupForecastGroup">
             <input
                    type="org.eclipse.worldcupforecast.WorldCupForecast">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.WorldCupForecast"
               tab="WorldCupForecastPLayer">
             <input
                    type="org.eclipse.worldcupforecast.WorldCupForecast">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.WorldCupForecast"
               tab="WorldCupForecastMatchDay">
             <input
                    type="org.eclipse.worldcupforecast.WorldCupForecast">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.WorldCupForecast"
               tab="WorldCupForecastPlayersRating">
             <input
                    type="org.eclipse.worldcupforecast.WorldCupForecast">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.GroupStageMatch"
               tab="GroupeStageMatch">
             <input
                    type="org.eclipse.worldcupforecast.GroupStageMatch">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.OtherKnockoutStageMatch"
               tab="OtherKnockoutStageGame">
             <input
                    type="org.eclipse.worldcupforecast.OtherKnockoutStageMatch">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.RoundOfSixteenMatch"
               tab="Base">
             <input
                    type="org.eclipse.worldcupforecast.RoundOfSixteenMatch">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.eclipse.worldcupforecast.section.ThirdPlaceMatch"
               tab="ThirdPlaceMatch">
             <input
                    type="org.eclipse.worldcupforecast.ThirdPlaceMatch">
             </input>
         </propertySection>
      </propertySections>
   </extension>
</plugin>
