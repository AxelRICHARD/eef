<?xml version="1.0" encoding="UTF-8"?>
<rm:rmap xmlns:bc="http://www.eclipse.org/buckminster/Common-1.0" xmlns:rm="http://www.eclipse.org/buckminster/RMap-1.0">
  <rm:property key="modeling.source.repository.location" value=":pserver:anonymous@dev.eclipse.org:/cvsroot/modeling"/>
  <rm:locator pattern="^org\.eclipse\.emf\.eef\.(?:releng|build)(?:\..+)?$" searchPathRef="releng"/>
  <rm:locator pattern="^org\.eclipse\.emf\.eef(?:\..+)?$" searchPathRef="eef"/>
  <rm:locator pattern="^org\.eclipse\.emf\.examples\.eef(?:\..+)?$" searchPathRef="eef"/>
  <rm:locator pattern="^org\.eclipse\.test$" searchPathRef="eclipse.test"/>
  <rm:locator pattern="^org\.eclipse\.ant\.optional\.junit$" searchPathRef="eclipse.test"/>
  <rm:searchPath name="eef">
    <rm:provider componentTypes="osgi.bundle" readerType="cvs" mutable="false">
      <rm:uri format="{0},org.eclipse.emf/org.eclipse.emf.eef/plugins/{1}">
        <bc:propertyRef key="modeling.source.repository.location"/>
        <bc:propertyRef key="buckminster.component"/>
      </rm:uri>
    </rm:provider>
    <rm:provider componentTypes="eclipse.feature" readerType="cvs" mutable="false">
      <rm:uri format="{0},org.eclipse.emf/org.eclipse.emf.eef/features/{1}-feature">
        <bc:propertyRef key="modeling.source.repository.location"/>
        <bc:propertyRef key="buckminster.component"/>
      </rm:uri>
    </rm:provider>
    <rm:provider componentTypes="osgi.bundle" readerType="cvs" mutable="false">
      <rm:uri format="{0},org.eclipse.emf/org.eclipse.emf.eef/doc/{1}">
        <bc:propertyRef key="modeling.source.repository.location"/>
        <bc:propertyRef key="buckminster.component"/>
      </rm:uri>
    </rm:provider>
    <rm:provider componentTypes="osgi.bundle" readerType="cvs" mutable="false">
      <rm:uri format="{0},org.eclipse.emf/org.eclipse.emf.eef/tests/{1}">
        <bc:propertyRef key="modeling.source.repository.location"/>
        <bc:propertyRef key="buckminster.component"/>
      </rm:uri>
    </rm:provider>
  </rm:searchPath>
  <rm:searchPath name="releng">
    <rm:provider componentTypes="buckminster" readerType="cvs" mutable="false">
      <rm:uri format="{0},org.eclipse.emf/org.eclipse.emf.eef/releng/{1}">
        <bc:propertyRef key="modeling.source.repository.location"/>
        <bc:propertyRef key="buckminster.component"/>
      </rm:uri>
    </rm:provider>
    <rm:provider componentTypes="eclipse.feature" readerType="cvs" mutable="false">
      <rm:uri format="{0},org.eclipse.emf/org.eclipse.emf.eef/releng/{1}-feature">
        <bc:propertyRef key="modeling.source.repository.location"/>
        <bc:propertyRef key="buckminster.component"/>
      </rm:uri>
    </rm:provider>
  </rm:searchPath>
  <rm:searchPath name="eclipse.test">
    <rm:provider componentTypes="osgi.bundle" readerType="cvs" mutable="false">
      <rm:uri format=":pserver:anonymous@dev.eclipse.org:/cvsroot/eclipse,{0}">
        <bc:propertyRef key="buckminster.component"/>
      </rm:uri>
    </rm:provider>
    <rm:provider componentTypes="eclipse.feature" readerType="cvs" mutable="false">
      <rm:uri format=":pserver:anonymous@dev.eclipse.org:/cvsroot/eclipse,{0}-feature">
        <bc:propertyRef key="buckminster.component"/>
      </rm:uri>
    </rm:provider>
  </rm:searchPath>
</rm:rmap>
