<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

 <plugin>

  <!-- EEF extensions -->
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.eclipse.org/emf/eef/mapping/filters/1.0.0"
            class="org.eclipse.emf.eef.filters.providers.FiltersEEFAdapterFactory"
            supportedTypes="org.eclipse.emf.eef.runtime.providers.PropertiesEditingProvider"/>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.eclipse.emf.eef.mapping.providers.MappingPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.eclipse.emf.eef.mapping.filters.providers.FiltersPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.eclipse.emf.eef.mapping.navigation.providers.NavigationPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>

   <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.eclipse.emf.eef.properties"
            labelProvider="org.eclipse.emf.eef.runtime.ui.utils.EEFLabelProvider">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.emf.eef.properties">
         <propertyTab
               label="Base"
               category="default"
               id="Base">
         </propertyTab>
         <propertyTab
               label="Documentation"
               category="extended"
               id="Documentation"
               afterTab="Base">
         </propertyTab>
         <propertyTab
               label="Filter properties"
               category="extended"
               id="Filter properties"
               afterTab="Filter properties">
         </propertyTab>
         <propertyTab
               label="Documentation"
               category="extended"
               id="Documentation"
               afterTab="Base">
         </propertyTab>
         <propertyTab
               label="Filter properties"
               category="extended"
               id="Filter properties"
               afterTab="Filter properties">
         </propertyTab>
         <propertyTab
               label="Documentation"
               category="extended"
               id="Documentation"
               afterTab="Base">
         </propertyTab>
         <propertyTab
               label="Filter properties"
               category="extended"
               id="Filter properties"
               afterTab="Filter properties">
         </propertyTab>
         <propertyTab
               label="Documentation"
               category="extended"
               id="Documentation"
               afterTab="Base">
         </propertyTab>
         <propertyTab
               label="Filter properties"
               category="extended"
               id="Filter properties"
               afterTab="Filter properties">
         </propertyTab>
         <propertyTab
               label="StrictTypingFilter"
               category="extended"
               id="StrictTypingFilter"
               afterTab="StrictTypingFilter">
         </propertyTab>
         <propertyTab
               label="Documentation"
               category="extended"
               id="Documentation"
               afterTab="Base">
         </propertyTab>
         <propertyTab
               label="Filter properties"
               category="extended"
               id="Filter properties"
               afterTab="Filter properties">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.eclipse.emf.eef.properties">
         <propertySection
               class="org.eclipse.emf.eef.mapping.filters.parts.forms.OnlyReferenceTypeFilterPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.OnlyReferenceTypeFilter"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.OnlyReferenceTypeFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.DocumentationPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.OnlyReferenceTypeFilter"
               tab="Documentation">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.OnlyReferenceTypeFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.FilterPropertiesPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.OnlyReferenceTypeFilter"
               tab="Filter properties">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.OnlyReferenceTypeFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.filters.parts.forms.OCLFilterPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.OCLFilter"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.OCLFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.DocumentationPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.OCLFilter"
               tab="Documentation">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.OCLFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.FilterPropertiesPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.OCLFilter"
               tab="Filter properties">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.OCLFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.filters.parts.forms.JavaExpressionFilterPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.JavaExpressionFilter"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.JavaExpressionFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.DocumentationPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.JavaExpressionFilter"
               tab="Documentation">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.JavaExpressionFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.FilterPropertiesPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.JavaExpressionFilter"
               tab="Filter properties">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.JavaExpressionFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.filters.parts.forms.JavaDeclarationFilterPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.JavaDeclarationFilter"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.JavaDeclarationFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.DocumentationPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.JavaDeclarationFilter"
               tab="Documentation">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.JavaDeclarationFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.FilterPropertiesPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.JavaDeclarationFilter"
               tab="Filter properties">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.JavaDeclarationFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.filters.parts.forms.StrictTypingFilterPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.StrictTypingFilter"
               tab="StrictTypingFilter">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.StrictTypingFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.DocumentationPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.StrictTypingFilter"
               tab="Documentation">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.StrictTypingFilter">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.FilterPropertiesPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.filters.section.StrictTypingFilter"
               tab="Filter properties">
             <input
                    type="org.eclipse.emf.eef.mapping.filters.StrictTypingFilter">
             </input>
         </propertySection>
      </propertySections>
   </extension>
 </plugin>
