<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

 <plugin>

  <!-- EEF extensions -->
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.eclipse.org/emf/eef/mapping/1.0.0"
            class="org.eclipse.emf.eef.mapping.providers.MappingEEFAdapterFactory"
            supportedTypes="org.eclipse.emf.eef.runtime.providers.PropertiesEditingProvider"/>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.eclipse.emf.eef.mapping.providers.MappingPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.eclipse.emf.eef.mapping.filters.providers.FiltersPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.eclipse.emf.eef.mapping.navigation.providers.NavigationPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>

   <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.eclipse.emf.eef.properties"
            labelProvider="org.eclipse.emf.eef.runtime.ui.utils.EEFLabelProvider">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.emf.eef.properties">
         <propertyTab
               label="Base"
               category="default"
               id="Base">
         </propertyTab>
         <propertyTab
               label="Documentation"
               category="extended"
               id="Documentation"
               afterTab="Base">
         </propertyTab>
         <propertyTab
               label="Documentation"
               category="extended"
               id="Documentation"
               afterTab="Base">
         </propertyTab>
         <propertyTab
               label="Documentation"
               category="extended"
               id="Documentation"
               afterTab="Base">
         </propertyTab>
         <propertyTab
               label="Documentation"
               category="extended"
               id="Documentation"
               afterTab="Base">
         </propertyTab>
         <propertyTab
               label="Documentation"
               category="extended"
               id="Documentation"
               afterTab="Base">
         </propertyTab>
         <propertyTab
               label="Documentation"
               category="extended"
               id="Documentation"
               afterTab="Base">
         </propertyTab>
         <propertyTab
               label="Documentation"
               category="extended"
               id="Documentation"
               afterTab="Base">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.eclipse.emf.eef.properties">
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.EMFElementBindingPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.EMFElementBinding"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.mapping.EMFElementBinding">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.views.parts.forms.DocumentationPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.EMFElementBinding"
               tab="Documentation">
             <input
                    type="org.eclipse.emf.eef.mapping.EMFElementBinding">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.EMFPropertyBindingPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.EMFPropertyBinding"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.mapping.EMFPropertyBinding">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.views.parts.forms.DocumentationPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.EMFPropertyBinding"
               tab="Documentation">
             <input
                    type="org.eclipse.emf.eef.mapping.EMFPropertyBinding">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.StandardElementBindingPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.StandardElementBinding"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.mapping.StandardElementBinding">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.views.parts.forms.DocumentationPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.StandardElementBinding"
               tab="Documentation">
             <input
                    type="org.eclipse.emf.eef.mapping.StandardElementBinding">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.StandardPropertyBindingPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.StandardPropertyBinding"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.mapping.StandardPropertyBinding">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.views.parts.forms.DocumentationPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.StandardPropertyBinding"
               tab="Documentation">
             <input
                    type="org.eclipse.emf.eef.mapping.StandardPropertyBinding">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.CategoryPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.Category"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.mapping.Category">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.views.parts.forms.DocumentationPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.Category"
               tab="Documentation">
             <input
                    type="org.eclipse.emf.eef.mapping.Category">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.ElementBindingReferencePropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.ElementBindingReference"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.mapping.ElementBindingReference">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.views.parts.forms.DocumentationPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.ElementBindingReference"
               tab="Documentation">
             <input
                    type="org.eclipse.emf.eef.mapping.ElementBindingReference">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.mapping.parts.forms.EMFMultiPropertiesBindingPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.EMFMultiPropertiesBinding"
               tab="Base">
             <input
                    type="org.eclipse.emf.eef.mapping.EMFMultiPropertiesBinding">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.views.parts.forms.DocumentationPropertiesEditionPartForm"
               id="org.eclipse.emf.eef.mapping.section.EMFMultiPropertiesBinding"
               tab="Documentation">
             <input
                    type="org.eclipse.emf.eef.mapping.EMFMultiPropertiesBinding">
             </input>
         </propertySection>
      </propertySections>
   </extension>
 </plugin>
